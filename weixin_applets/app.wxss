}
.quantity .minus.disabled:before {
  background-color: #dedede;
}
.quantity .plus.disabled:after, .quantity .plus.disabled:before {
  background-color: #ddd;
}
.quantity .plus:after {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  width: 5rpx;
  height: 30rpx;
  margin: auto;
  background-color: #fff;
}
